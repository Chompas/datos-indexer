#include "DirectoryReader.h"
#include <string.h>

DirectoryReader* DirectoryReader::instance = 0;

DirectoryReader::DirectoryReader() {

}

DirectoryReader* DirectoryReader::getInstance() {
	if (instance == 0) {
		instance = new DirectoryReader();
	}
	return instance;
}



